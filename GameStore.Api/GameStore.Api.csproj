<!-- this means that it's a web project, by default to import a bunch of libraies and dependencies-->
<Project Sdk="Microsoft.NET.Sdk.Web">

  
  <PropertyGroup>
    <!-- here means that we have the acesses to all of the apis that are availble in version 8 of the net SDK -->
    <TargetFramework>net8.0</TargetFramework>
    <!-- dependencies and libs -->
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

</Project>
